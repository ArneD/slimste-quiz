                <TextBlock Style="{StaticResource ScoreText}">13</TextBlock>
            </Label>
        </Viewbox>
        <Viewbox Grid.RowSpan="2" Grid.Column="13" Margin="0,-15,0,0">
            <Label Style="{StaticResource PointsEllipseStyle}" IsEnabled="{Binding IsSelected[13]}">
                <TextBlock Style="{StaticResource ScoreText}">14</TextBlock>
            </Label>
        </Viewbox>
        <Viewbox Grid.RowSpan="2" Grid.Column="14" Margin="0,-45,0,0">
