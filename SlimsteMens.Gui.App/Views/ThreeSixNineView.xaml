                <TextBlock Style="{StaticResource ScoreText}">11</TextBlock>
            </Label>
        </Viewbox>
        <Viewbox Grid.RowSpan="2" Grid.Column="11" Margin="0,45,0,0">
            <Label Style="{StaticResource PointsEllipseStyle}" IsEnabled="{Binding IsSelected[11]}">
                <TextBlock Style="{StaticResource ScoreText}">12</TextBlock>
            </Label>
        </Viewbox>
        <Viewbox Grid.RowSpan="2" Grid.Column="12" Margin="0,15,0,0">
