                <TextBlock Style="{StaticResource ScoreText}">15</TextBlock>
            </Label>
        </Viewbox>
        <MediaElement Source="Audio/Welkom.wma" LoadedBehavior="Manual" Visibility="Collapsed" Volume="1" Name="AudioElement"></MediaElement>
        <ContentPresenter Grid.ColumnSpan="20" Grid.Row="5" Content="{Binding ScoreView}" />
    </Grid>
</UserControl>
